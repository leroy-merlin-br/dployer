{
    "name": "leroy-merlin-br/dployer",
    "description": "A simple script to deploy PHP applications to AWS ElasticBeanstalk",
    "license": "MIT",
    "keywords": [
        "deploy",
        "aws",
        "elasticbeanstalk",
        "ebs"
    ],
    "require": {
        "aws/aws-sdk-php": "~2.6.5",
        "icecave/isolator": "~2.2.1",
        "illuminate/container": "~4.2.0-BETA1",
        "symfony/console": "~2.5"
    },
    "autoload": {
        "classmap": [
            "app"
        ]
    },
    "bin": [
        "dployer"
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
